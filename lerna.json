
{
  "command": {
    "version": {
      "conventionalCommits": true,
      "changelogPreset": "angular-all",
      "message": "chore(release): publish %s"
    }
  },
  "publish": {
    "access": "public",
    "allowBranch": "master",
    "ignoreChanges": [
      "CHANGELOG.md"
    ]
  },
  "version": "independent",
  "npmClient": "yarn",
  "packages": ["."]
}